{
  "name": "backend",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "NODE_ENV=production node ./bin/www.js",
    "test": "NODE_ENV=test ENABLE_LOGGING=false HIDE_ENV_VARIABLES=false DEBUG=true CLEAR_STARTING_LOGS=true ./node_modules/.bin/mocha -s 350 --require ./test/hooks.js --exit",
    "test-junit": "NODE_ENV=test ENABLE_LOGGING=false HIDE_ENV_VARIABLES=false DEBUG=true CLEAR_STARTING_LOGS=true ./node_modules/.bin/nyc ./node_modules/.bin/mocha -s 350 --require ./test/hooks.js --exit --reporter mocha-junit-reporter --reporter-options mochaFile=reports/mocha/test-results.xml",
    "test-codecov": "./node_modules/.bin/nyc report --reporter=lcov",
    "coverage": "./node_modules/.bin/codecov",
    "docs": "./node_modules/jsdoc/jsdoc.js -c ./.jsdoc.conf.json --readme ../../../README.md"
  },
  "jest": {
    "testEnvironment": "node",
    "verbose": true
  },
  "dependencies": {
    "axios": "^0.21.1",
    "chai": "^4.3.4",
    "chalk": "^4.1.1",
    "cookie-parser": "~1.4.4",
    "dotenv": "^8.6.0",
    "ethers": "^5.3.0",
    "express": "~4.16.1",
    "express-validator": "^6.11.1",
    "http-errors": "~1.6.3",
    "i18next": "^20.3.0",
    "i18next-express-middleware": "^2.0.0",
    "i18next-node-fs-backend": "^2.1.3",
    "ip": "^1.1.5",
    "lodash": "^4.17.21",
    "morgan": "~1.9.1",
    "public-ip": "^4.0.3",
    "sawtooth-sdk": "^1.0.5",
    "secp256k1": "^4.0.2"
  },
  "devDependencies": {
    "better-docs": "^2.3.2",
    "chai-http": "^4.3.0",
    "codecov": "^3.8.2",
    "eslint": "^7.27.0",
    "eslint-config-prettier": "^8.3.0",
    "jsdoc": "^3.6.7",
    "mocha": "^8.4.0",
    "mocha-junit-reporter": "^2.0.0",
    "nyc": "^15.1.0",
    "swagger-ui-express": "^4.1.6",
    "uuid": "^8.3.2"
  }
}
